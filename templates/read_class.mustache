<p>Class: <b>{{resource_name}}</b></p>

{{#no_content}}
  <p>This class does not have any documentation yet.</p>

  <a href="{{url_edit}}">Create class</a>
{{/no_content}}


{{#has_content}}

  {{> read_synopsis }}

  {{#has_base}}
    <p>Base class: {{base}}</p>
  {{/has_base}}

  {{> read_module_includes }}

  {{> read_module_constants }}

  {{> read_details }}

  {{> read_module_cmethods }}

  {{> read_module_imethods }}

  <a href="{{url_edit}}">Edit class</a>

{{/has_content}}
